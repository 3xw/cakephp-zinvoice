{
  "name": "3xw/cakephp-zinvoice",
  "description": "CakePHP webservice implementation for Zoho Invoice",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.5.9",
    "cakephp/cakephp": ">=3.3.2 <4.0.0",
    "muffin/webservice": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "3xw\\Zinvoice\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "3xw\\Zinvoice\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
